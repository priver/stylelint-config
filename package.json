    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.5",
    "stylelint": "^9.2.0",
    "stylelint-order": "^0.8.1"
